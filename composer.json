{
    "name": "studiobonito/Cookie-Bar",
        "description": "Simple EU cookies law consent bar",
    "type": "silverstripe-vendormodule",
    "keywords": ["silverstripe", "cookie"],
    "authors": [
        {
            "name": "Aram Balakjian",
            "email": "aram@carboncrayon.com"
        },
        {
            "name": "Steve Heyes",
            "email": "steve.heyes@studiobonito.co.uk"
        }
    ],
    "require": {
        "silverstripe/framework": "^4"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "studiobonito\\Cookie-Bar\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "studiobonito\\Cookie-Bar\\Tests\\": "tests/"
        }
    },
    "prefer-stable": true
}
